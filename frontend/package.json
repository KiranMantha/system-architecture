{
  "name": "plumejs-vite-template",
  "version": "1.0.0",
  "description": "PlumeJS template with vite bundler",
  "main": "index.js",
  "scripts": {
    "generate-runtime-env": "cross-env NODE_ENV=development runtime-env-cra --env-file=./.env",
    "start": "yarn generate-runtime-env --config-name=./src/runtime-env.js && vite",
    "build": "vite build && yarn generate-runtime-env --config-name=./dist/runtime-env.js",
    "serve": "yarn build && serve -s dist",
    "test": "vitest run --coverage",
    "lint": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@plumejs/core": "^4.0.2",
    "cross-env": "^7.0.3",
    "runtime-env-cra": "^0.2.4",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "c8": "^9.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "happy-dom": "^13.3.8",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^1.2.2"
  }
}
